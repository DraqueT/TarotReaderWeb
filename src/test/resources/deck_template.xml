<!--Do not create or distribute digital decks for which you do not have permission or the rights. Seriously.-->
<Deck>
    <DeckAttributes>
        <DeckName>
            
        </DeckName>
        
        <!--Uniqueness required - AUTOGENERATED GUID -->
        <DeckId>
            
        </DeckId>
        
        <DeckDescription>
            
        </DeckDescription>
        
        <!--ISO 639-2 Code-->
        <Language>
            eng
        </Language>
        
        <!--This is where your name goes as the creator of this file-->
        <FileAuthor>
            
        </FileAuthor>

        <FileCopyrightDate>
            
        </FileCopyrightDate>
 
        <!--This is where the creator of the deck gets credit-->
        <DeckAuthor>
            
        </DeckAuthor>
        <DeckCopyrightDate>
            
        </DeckCopyrightDate>
        
        <!--Image type as element attribute-->
        <DeckBackAsset type='png'>
            <!--DECK BACK RELATIVE LOCATION HERE-->
        </DeckBackAsset>
    </DeckAttributes>
    
    <!-- Keywords on cards can be matched to larger descriptions/explanations here -->
    <KeywordDescriptions>
    	<KeywordDescription>
    		<DeckKeyword>
    		</DeckKeyword>
    		<DeckKeywordText>
    		</DeckKeywordText>
    	</KeywordDescription>
    </KeywordDescriptions>
    
    <!--Define suits of your deck here. Arcana is stored as a suit, but marked with the arcana = true attribute-->
    <Suits>
        <Suit arcana="true">
            <SuitName>
                
            </SuitName>
            
            <!--Uniqueness requied (must be a positive number)-->
            <SuitId>
                
            </SuitId>
            
            <!--this is the description which will be given the first time the suit appears in a reading-->
            <ShortDescription>
            </ShortDescription>
            
            <SuitDescription>
            </SuitDescription>
        </Suit>
    </Suits>
    
    <Cards>
        <Card court="false">
            <!--Combination of card number and suit must be unique - number must be positive-->
            <CardNumber>
            </CardNumber>
            <!--The suit id must match one of the suits defined under suits (will be a positive number)-->
            <CardSuitId>
            </CardSuitId>
            
            <CardName>
            </CardName>
            
            <!--Optional. If not present, defualts to long description.-->
            <ShortDescription/>
            
            <LongDescription>
            </LongDescription>
            
            <ReadingText>
            </ReadingText>
            
            <!--Optional. Reading text when inverted. If not present, defaults to regular reading text.-->
            <InvertedReadingText/>
            
            <CardFrontAsset type='png'>
            </CardFrontAsset>
            
            <!--These are relations to other cards and positions. Text appended within readings as appropriate-->
            <CardReadingRelations>
            	<!-- Comma delimited list of keywords. Cards with matching keywords may be auto-related to one another in readings active. -->
          		<CardKeywords>
    		    </CardKeywords>
            
                <!--Ids of suits this card is related to (useful in decks where arcana can have multiple suit/element relations)-->
                <RelatedSuits>
                    <!--numeric suitid-->
                    <Suit></Suit>
                </RelatedSuits>
                
                <!--When already present in a reading, these cards will cause the parent card to append the reading text to its reading.-->
                <RelatedCards>                
                    <CardRelation>
                        <CardNumberRelation>
                            
                        </CardNumberRelation>
                        <CardSuitRelation>
                            
                        </CardSuitRelation>
                        <ReadingText>
                            
                        </ReadingText>
                        <InvertedReadingText>
                        </InvertedReadingText>
                    </CardRelation>
                </RelatedCards>
                
                <!--Relationships to specific spread positions (unique per spread)-->
                <!--Text appended to reading of card when parent card is in the specified position-->
                <PositionRelations>
                    <PositionRelation>
                        <SpreadIdRelation>
                        </SpreadIdRelation>
                        
                        <PositionIdRelation>
                            
                        </PositionIdRelation>
                        <ReadingText>
                            
                        </ReadingText>
                        <InvertedReadingText>
                        </InvertedReadingText>
                    </PositionRelation>
                </PositionRelations>
            </CardReadingRelations>
        </Card>
    </Cards>
</Deck>
